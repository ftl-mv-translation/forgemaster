#: src-en/data/hyperspace.xml.append:1884
msgid ""
"data/hyperspace.xml.append$/dummyroot/FTL[4]/mod:findLike/mod-"
"append:shipIcons/shipIcon[16]/tooltip"
msgstr "電磁船体体：システム損傷に抵抗する10％の確率、イオン損傷に抵抗する30％の確率、脱イオン化速度 +50％"

#: src-en/data/hyperspace.xml.append:1889
msgid ""
"data/hyperspace.xml.append$/dummyroot/FTL[4]/mod:findLike/mod-"
"append:shipIcons/shipIcon[17]/tooltip"
msgstr ""
