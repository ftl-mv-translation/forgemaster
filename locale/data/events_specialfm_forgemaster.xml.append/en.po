"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_HYDROPONICS_HUB\"]/choice[2]/text"
msgstr "Leave Hydroponics for now."

#: src-en/data/events_specialfm_forgemaster.xml.append:13786
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_HYDROPONICS_HUB\"]/choice[@req=\"pilot\""
