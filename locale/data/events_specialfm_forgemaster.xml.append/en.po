#: src-en/data/events_specialfm_forgemaster.xml.append:14217
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_COALITION_STORAGE_CRATES\"]/choice[@req=\"marker_need_rope\"]/text"
msgstr "Open a crate labelled \"Rope, Twine, and String\"."

#: src-en/data/events_specialfm_forgemaster.xml.append:14222
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_COALITION_STORAGE_CRATES\"]/choice[@req=\"marker_rope_obtained\""
" and @lvl=\"0\"]/text"
msgstr ""
"You have no known need for rope at this time, you decide to leave it be."

#: src-en/data/events_specialfm_forgemaster.xml.append:14227
msgid ""
"data/events_specialfm_forgemaster.xml.append$//event[@name=\"FM_FS_COALITION_STORAGE_CRATES\"]/choice[@req=\"pilot\"]/text"
